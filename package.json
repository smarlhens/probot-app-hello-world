{
  "name": "probot-app-hello-world",
  "version": "1.0.0",
  "private": true,
  "description": "A Probot app",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "homepage": "https://github.com//",
  "license": "ISC",
  "author": "Samuel MARLHENS",
  "scripts": {
    "build": "tsc",
    "lint": "npm run prettier:check",
    "lint-staged": "lint-staged",
    "precommit": "npm run lint-staged && npm run build",
    "prepare": "husky install",
    "prettier:check": "prettier --check ./**/*.{ts,json,yml,md}",
    "prettier:write": "prettier --write ./**/*.{ts,json,yml,md}",
    "start": "probot run ./lib/index.js",
    "test": "vitest run",
    "test:ci": "npm run test:coverage",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "probot": "12.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/node": "18.11.18",
    "@vitest/coverage-c8": "0.28.3",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "nock": "13.3.0",
    "prettier": "2.8.3",
    "smee-client": "1.2.3",
    "sort-package-json": "2.3.0",
    "typescript": "4.9.5",
    "vitest": "0.28.3"
  },
  "engines": {
    "node": "^16.13.0 || >=18.0.0",
    "npm": ">=8.0.0"
  }
}
